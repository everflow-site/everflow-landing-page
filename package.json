    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-recommended": "4.1.0",
    "framer-motion": "4.1.17",
    "jotai-immer": "^0.3.0",
    "lodash": "4.17.21",
    "query-string": "7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "2.0.4",
    "react-hot-toast": "2.2.0",
    "react-icons": "4.3.1",
    "react-loading-skeleton": "3.3.1",
    "react-redux": "^9.1.0",
